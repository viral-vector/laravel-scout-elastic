{
    "name": "viral-vector/laravel-scout-elastic",
    "description": "Elastic Driver for Laravel Scout",
    "homepage": "https://github.com/viral-vector/laravel-scout-elastic",
    "license": "MIT",
    "keywords": ["laravel", "scout", "elasticsearch", "elastic"],
    "require": {
        "php": ">=7.1.3",
        "laravel/scout": "^5.0|^6.0|^7.0",
        "elasticsearch/elasticsearch": "^6.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "ViralVector\\LaravelScoutElastic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ViralVector\\LaravelScoutElastic\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "elasticsearch/elasticsearch": "Required to use the Elasticsearch engine (^6.0)."
    },
    "extra": {
        "laravel": {
            "providers": [
                "ViralVector\\LaravelScoutElastic\\LaravelScoutElasticServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
